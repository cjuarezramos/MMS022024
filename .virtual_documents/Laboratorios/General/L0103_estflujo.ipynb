def fib(n):    # escribe la serie de Fibonacci hasta el número n
    """Imprime la serie de fibonnaci."""
    a, b = 0, 1
    while a < n:
        print(a, end=' ') # end=' ' elimina salto de línea
        a, b = b, a+b
    print()


# Llamemos a la función
fib(2000)


fib('spam')



