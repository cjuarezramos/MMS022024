import math

tol = 0.01
error = 1
x = 2
sum = 0
n = 0
sum = x**n/math.factorial(n)
while error > tol:
    n = n + 1
    val_ant = sum
    sum = sum + x**n/math.factorial(n)
    val_nuevo = sum
    error = abs((val_nuevo-val_ant)/val_nuevo)

print(val_nuevo)
    
    
    


